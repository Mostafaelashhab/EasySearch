{
    "name": "mostafaelashhab/easy-search",
    "description": "A custom search package for Laravel",
    "autoload": {
        "psr-4": {
            "Mostafaelashhab\\EasySearch\\": "src/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "mostafaelashhab/easy-search": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaelashhab\\EasySearch\\SearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

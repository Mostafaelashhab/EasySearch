{
  "name": "mostafaelashhab/easy-search",
  "description": "A custom search package for Laravel",
  "autoload": {
    "psr-4": {
      "Mostafaelashhab\\EasySearch\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mostafaelashhab\\EasySearch\\SearchServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mostafaelashhab/easy-search"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
